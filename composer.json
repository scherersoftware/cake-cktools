{
    "name": "codekanzlei/cake-cktools",
    "description": "codekanzlei Tools & Utilities collection",
    "license": "MIT",
    "type": "cakephp-plugin",
    "extra": {
        "installer-name": "CkTools"
    },
    "require": {
        "cakephp/plugin-installer": "*",
        "facebook/php-sdk-v4": "4.0.*",
        "mpdf/mpdf": "6.1.0"
    },
    "require-dev": {
        "cakephp/cakephp": "3.0.x-dev"
    },
    "autoload": {
        "psr-4": {
        "CkTools\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
